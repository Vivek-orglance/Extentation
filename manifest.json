{
    "manifest_version": 3,
    "name": "Web Scraper Extension",
    "version": "1.0",
    "description": "Scrape data from a specific webpage.",
    "permissions": ["scripting", "activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon.png"
      }
    },
    "icons": {
      "16": "images/icon.png"
    }
  }
  